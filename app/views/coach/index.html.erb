<h1 class="h2 text-center">Available Coaches</h1>
<div class="text-center3 pt-4">
  <div class="row">
    <div class="col-6 offset-3 text-center">
      <% @coaches.each do |coach| %>
        <div class="p-1"><%= link_to coach.name, coach_show_path(coach.id), class: 'coach' %></div>
      <% end %>
    </div>
  </div>
</div>

